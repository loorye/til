# デモ用: true の場合は外部APIを呼ばずに擬似レスポンスを返す
MOCK_MODE=true
NEXT_PUBLIC_MOCK_MODE=true

# Basic認証設定（本番環境では必ず変更してください）
AUTH_USERNAME=admin
AUTH_PASSWORD=your-secure-password
# DISABLE_AUTH=true # 認証を無効化する場合（ローカル開発時など）

# API keys (MOCK_MODE=false の場合に設定)
OPENAI_API_KEY=
GOOGLE_API_KEY=
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_SESSION_TOKEN= # optional (assume roleの場合)

# Optional model overrides
OPENAI_MODEL=gpt-4o-mini
BEDROCK_REGION=us-east-1
BEDROCK_MODEL_ID=anthropic.claude-3-5-sonnet-20240620-v1:0
GEMINI_MODEL=gemini-1.0-pro
